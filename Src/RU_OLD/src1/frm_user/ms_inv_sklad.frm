  fRoundRubOrd(1,psumP)  // количество порядковых номеров прописью
  if (abs(pnum) >= GetPresision_forKol,      DoubleToStr(pnum,'\m 4'),      '' )  // общее количество единиц фактически прописью
  if (abs(pkolF) >= GetPresision_forKol,      DoubleToStr(Round(pkolF,5),'\m 4 целых,\m 5'),      '' )  // на сумму фактически прописью
  if (abs(psumF) >= 0.01, DoubleToStr(fRoundRubOrd(1,psumF),'\m 4 руб'), '' )
  'true':'s'
  if (abs(rnum) >= GetPresision_forKol,      DoubleToStr(rnum,'666`666`666`666`666'),      '' )  // фактическое количество (по отчету)
  if (abs(rkolF) >= GetPresision_forKol,      DoubleToStr(rkolF,'[|-]3666`666`666`666`666'+sKolZnak),      '' )  // фактическая сумма (по отчету)
  fRoundRubOrd(1,rsumF)  // программное количество (по отчету)
  if (abs(rkolP) >= GetPresision_forKol,      DoubleToStr(rkolP,'[|-]3666`666`666`666`666'+sKolZnak),      '' )  // программная сумма (по отчету)
  fRoundRubOrd(1,rsumP)  // количество порядковых номеров прописью (по отчету)
  if (abs(rnum) >= GetPresision_forKol,      DoubleToStr(rnum,'\m 4'),      '' )  // общее количество единиц фактически прописью (по отчету)
  if (abs(rkolF) >= GetPresision_forKol,      DoubleToStr(Round(rkolF,5),'\m 4 целых,\m 5'),      '' )  // на сумму фактически прописью (по отчету)
  if (abs(rsumF) >= 0.01, DoubleToStr(fRoundRubOrd(1,rsumF),'\m 4 руб'), '' )
  Председатель_комиссии_Должность
  Председатель_комиссии_ФИО
  if (Номер_члена_комиссии = word(1), 'Члены комиссии:', '')
  Массив_Член_комиссии_Должность [Номер_члена_комиссии]
  Массив_Член_комиссии_ФИО       [Номер_члена_комиссии]
  мол
.endfields
.begin
