          Result := FALSE;
          Message('Д'+'е'+'м'+'о'+'-'+'р'+'е'+'ж'+'и'+'м пре'+'дпо'+'лаг'+'ает нал'+'ичие не бол'+'ее ' + string(CalcSum_MaxSum#CalcSum_func5(D,ogo,vo,r))
                 +' '+'дого'+'во'+'ров', CancelButton);
          break;
        }
    }
}


