Function CalcSum_#CalcSum_func5(D,ogo,vo,r)(wVidDoc: word; dx : byte) : boolean;
{
  wVidDoc := wVidDoc;

  Result := TRUE;

  var lCount : word;
  lCount := 0;

  vCalcSum._LOOP #CalcSum_func5(D,ogo,vo,r)
    {
      lCount++;

      if (lCount > CalcSum_MaxSum#CalcSum_func5(D,ogo,vo,r) - dx)
